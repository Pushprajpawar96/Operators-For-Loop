import java.util.Scanner;
public class Main {
	
	public static void main(String[] args) {
Scanner sc = new Scanner(System.in);
		  long bn, dn = 0, j = 1, remainder;
//		  System.out.print("");
		  bn = sc.nextLong();

		  while (bn!= 0) 
		  {
		   remainder = bn % 10;
		   dn = dn + remainder * j;
		   j = j * 2;
		   bn = bn / 10;
		  }
		  System.out.println( dn);
		 }		

	}
